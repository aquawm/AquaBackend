<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AquaBackend</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">

                <img src="~/Images/LogoAqua.png" alt="" height="75" />
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Reportes</a>
                            <div class="dropdown-menu">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Reports" asp-action="PortfolioReports">Portfolios</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Reports" asp-action="GeneralReports">Generales</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Reports" asp-action="FundFactsheetReports">Factsheet de Fondos</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Reports" asp-action="FundPerformanceReports">Performance Review de Fondos</a>
                            </div>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Información</a>
                            <div class="dropdown-menu submenu">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Transactions" asp-action="Transactions">Transacciones</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Prices" asp-action="Prices">Precios</a>
                                <a class="nav-link text-dark" asp-area="" asp-controller="APXUserSessions" asp-action="Index">Sesiones de APX</a>
                            </div>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="MarketSummaries" asp-action="ViewMarketSummaries">Resumen del Mercado</a>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Accesos Directos</a>
                            <div class="dropdown-menu">

                                <a class="nav-link text-dark" target="_blank" href="https://apx.aquawm.com/apxuilogin">
                                    APX
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="http://192.168.1.9:82">
                                    Aqua Private Equity
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="https://app.powerbi.com">
                                    Power BI
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="http://192.168.1.8/wiki">
                                    Aqua Wiki
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="https://online.aquawm.com/auth/login">
                                    Aqua Online
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="https://aquawmbr.dyndns.org:8094/">
                                    Control Tower
                                </a>

                                <a class="nav-link text-dark" target="_blank" href="https://aqua.crm2.dynamics.com/">
                                    CRM
                                </a>

                            </div>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Configuración</a>
                            <div class="dropdown-menu">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Clients" asp-action="Index">Clientes</a>
                            </div>
                        </li>

                    </ul>
                </div>
            </div>


        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>

    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - AquaBackend

        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
